									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="informationSourceName" minOccurs="0">
					<xs:complexType>
						<xs:choice>
							<xs:element name="personName" type="gcda:pnm"/>
							<xs:element name="organizationName" type="gcda:onm"/>
							<xs:element name="relatedPersonName" type="gcda:pnm"/>
